{
    "name": "nawhas/nawhas",
    "type": "project",
    "description": "Nawhas.com",
    "license": "MIT",
    "require": {
        "php": "7.4.*",
        "acelaya/doctrine-enum-type": "^2.3",
        "beberlei/doctrineextensions": "^1.2",
        "fideloper/proxy": "^4.0",
        "gedmo/doctrine-extensions": "^2.4",
        "graham-campbell/github": "^9.1",
        "intervention/image": "^2.5",
        "laravel-doctrine/extensions": "^1.1",
        "laravel-doctrine/fluent": "^1.1",
        "laravel-doctrine/migrations": "^1.3",
        "laravel-doctrine/orm": "^1.5",
        "laravel/airlock": "^1.0",
        "laravel/framework": "^6.2",
        "laravel/socialite": "^4.4",
        "laravel/tinker": "^2.0",
        "lcobucci/jwt": "^3.3",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/fractal": "^0.19.2",
        "myclabs/php-enum": "^1.7",
        "php-http/guzzle6-adapter": "^2.0",
        "ramsey/uuid-doctrine": "^1.6",
        "sentry/sentry-laravel": "1.7.0",
        "socialiteproviders/apple": "^3.0",
        "spatie/laravel-cors": "^1.6",
        "timacdonald/has-parameters": "^1.0",
        "weirdan/doctrine-psalm-plugin": "^0.10.0",
        "zain/laravel-doctrine-algolia": "^0.1.0",
        "zain/laravel-doctrine-jetpack": "dev-master"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3",
        "nunomaduro/larastan": "^0.5.2",
        "phpstan/phpstan": "^0.12.11",
        "phpunit/phpunit": "^9.0",
        "psalm/plugin-laravel": "^1.1",
        "vimeo/psalm": "^3.9"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "phpstan": [
            "./vendor/bin/phpstan analyze --memory-limit=2G"
        ],
        "psalm:plugin": [
            "./vendor/bin/psalm-plugin"
        ],
        "psalm:lint": [
            "./vendor/bin/psalm"
        ],
        "test": [
            "./vendor/bin/phpunit --colors=always"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
